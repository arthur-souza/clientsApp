<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ClientInfo">

    <TextView
            android:text="CPF:"
            android:layout_width="wrap_content"
            android:layout_height="16dp"
            tools:ignore="MissingConstraints"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp" android:layout_marginTop="15dp"
            app:layout_constraintTop_toBottomOf="@+id/textView7" android:id="@+id/textView9"
            android:textColor="@color/colorAccent"/>
    <TextView
            android:text="texto"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:ignore="MissingConstraints" android:id="@+id/txt_name"
            android:layout_marginLeft="6dp" android:layout_marginStart="6dp"
            app:layout_constraintStart_toEndOf="@+id/textView7" android:layout_marginTop="36dp"
            app:layout_constraintTop_toTopOf="parent"/>

    <TextView
            android:text="DATA DE NASCIMENTO:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:ignore="MissingConstraints"
            android:layout_marginTop="19dp"
            app:layout_constraintTop_toBottomOf="@+id/txt_cpf" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp" android:id="@+id/textView8"
            android:textColor="@color/colorAccent"/>
    <TextView
            android:text="texto"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:ignore="MissingConstraints" android:id="@+id/txt_cpf"
            android:layout_marginTop="15dp"
            app:layout_constraintTop_toBottomOf="@+id/txt_name" app:layout_constraintStart_toEndOf="@+id/textView9"
            android:layout_marginLeft="14dp" android:layout_marginStart="14dp"/>

    <TextView
            android:text="NOME:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:ignore="MissingConstraints"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="36dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp" android:id="@+id/textView7" android:textColor="@color/colorAccent"/>
    <TextView
            android:text="texto"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:ignore="MissingConstraints" android:id="@+id/txt_dateBirth"
            android:layout_marginLeft="17dp" android:layout_marginStart="17dp"
            app:layout_constraintStart_toEndOf="@+id/textView8" android:layout_marginTop="108dp"
            app:layout_constraintTop_toTopOf="parent"/>
</android.support.constraint.ConstraintLayout>