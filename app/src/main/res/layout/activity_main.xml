<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">

    <Button
            android:text="CADASTRAR"
            android:layout_width="160dp"
            android:layout_height="60dp"
            android:id="@+id/btn_register" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="200dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="112dp" android:layout_marginStart="112dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="112dp"
            android:layout_marginRight="112dp"
            app:layout_constraintHorizontal_bias="0.0"/>
    <Button
            android:text="CLIENTES"
            android:layout_width="160dp"
            android:layout_height="60dp"
            android:id="@+id/btn_clients" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="272dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="112dp" android:layout_marginStart="112dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="112dp"
            android:layout_marginRight="112dp"
            app:layout_constraintHorizontal_bias="0.0"/>
</android.support.constraint.ConstraintLayout>